   part_wpn_melee_heavy__name:
     temp: true
-    text: Heavy melee
+    text: 重量級近接武器
   part_wpn_melee_heavy__text:
     temp: true
-    text: Large melee weapons typically reserved for primary weapon slot.
+    text: 通常、主兵装スロットに割り当てられる大型近接武器。
   part_wpn_melee_light__name:
     temp: true
-    text: Light melee
+    text: 軽量級近接武器
   part_wpn_melee_light__text:
     temp: true
-    text: Smaller melee weapons compatible with secondary weapon slot.
+    text: 副兵装スロットと互換性のある小型近接武器。
   part_wpn_melee_medium__name:
     temp: true
-    text: Medium melee
+    text: 中量級近接武器
